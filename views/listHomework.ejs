<!DOCTYPE html>
<% include studentHeader.ejs %>
<div class="container services">
<div class="jumbotron" >
<h1><%= title %></h1>        
</div>
<div class="container">
    <div class="row">
        <div class="span5">
            <table class="table table-striped table-condensed">
                <thead>
                    <tr>
                        <th>作業名稱</th>
                        <th>上傳情形</th>
                        <th>繳交期限</th>
                        <th>繳交時間</th>
                        <th>作業成績</th>       
                        <th>上傳</th>                                   
                    </tr>
                </thead>   
                <tbody>
                    <tr>
                        <%  for(let i=0; i < result.homeworks.length; i++) { %>
                        <td><%= result.homeworks[i].homeworkName %></td>
                        <td><%= result.grades[i].homeworkState %></td>
                        <td><%= result.homeworks[i].dueDate %></td>
                        <td><%= result.grades[i].submitTime %></td>
                        <td><%= result.grades[i].homeworkGrade %></td>
                        <td><a class="btn btn-info" href="/listHomework/uploadHomework?homework_uuid=<%= result.homeworks[i]._id %>&studentID=<%= result.studentID %>" >上傳作業</a></td>                                       
                    </tr>
                    <% } %>             
                </tbody>
            </table>
        </div>
    </div>
</div>
</div>
<% include footer.ejs %>
