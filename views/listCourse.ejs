<!DOCTYPE html>
<% include header.ejs %>

<div class="container services">
<div class="jumbotron" >
<h1><%= title%></h1>        
</div>
<div class="container">
    <div class="row">
        <div class="span5">
            <table class="table table-striped table-condensed">
                <thead>
                    <tr>
                        <th>課堂編號</th>
                        <th>課堂名稱</th>
                        <th>教室</th>
                        <th>授課教師</th>
                        <th>進入教室</th>                                          
                    </tr>
                </thead>   
                <tbody>
                    <%  for(var i=0; i < result.length; i++) { %>
                    <tr>
                        <td><%= result[i].courseID %></td>
                        <td><%= result[i].courseName %></td>
                        <td><%= result[i].classroom %></td>
                        <td><%= result[i].instructor %></td>
                        <td><a class="btn btn-info" href="/listCourse/listHomework?courseName=<%= result[i].courseName %>&studentID=105598001">進入</a></td>                                       
                    </tr>
                    <% } %>           
                </tbody>
            </table>
        </div>
    </div>
</div>
</div>
<% include footer.ejs %>