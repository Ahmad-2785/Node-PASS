<% include header.ejs %>
<div class="container services">
<div class="well jumbotron">
<h1><%= title %></h1>
</div>
    <table class="table table-striped table-condensed">
        <thead>
            <tr>
                <th>學號</th>
                <th>姓名</th>
                <th>成績</th>
                <th>繳交時間</th>
                <th>批改作業</th>                                          
            </tr>
        </thead>   
        <tbody>
            <% for(var i=0; i < result.length; i++) { %>
            <tr>
                <td id="<%= result[i].studentID %>" ><%= result[i].studentID %></td>
                <td><%= result[i].studentName %></td>
                <td id="grade_<%= result[i].studentID %>" ><%= result[i].homeworkGrade %></td>
                <td><%= result[i].submitTime %></td>
                <td><a class="btn btn-info" id="toMark_<%= result[i].studentID %>" href="/markHomework/person/info/<%= result[i].studentID %>/<%= result[i].homework_uuid %>" title="<%= result[i].studentID %>">批改</a></td><!-- href到個人 -->
            </tr>
            <% } %>
        </tbody>
    </table>
</div>
<% include footer.ejs %>