<% include header.ejs %>
<div class="container services">
<div class="well jumbotron">
<h1><%= title %></h1>
</div>
    <form action="">
        <div class="container">
            <div class="row">
                <div class="span5">
                    <table class="table table-striped table-condensed">
                        <thead>
                            <tr>
                                <th>學號</th>
                                <th>姓名</th>
                                <th>成績</th>
                                <th>繳交時間</th>
                                <th>批改作業</th>                                          
                            </tr>
                        </thead>   
                        <tbody>
                            <% for(var i=0; i < result.length; i++) { %>
                            <tr>
                                <td><%= result[i].studentID %></td>
                                <td><%= result[i].studentName %></td>
                                <td><%= result[i].homeworkGrade %></td>
                                <td><%= result[i].submitTime %></td>
                                <td><a class="btn btn-info" href="/markHomework/person/info/<%= result[i].studentID %>" title="<%= result[i].studentID %>">批改</a></td><!-- href到個人 -->
                            </tr>
                            <% } %>
                        </tbody>
                    </table>
                </div>
            </div>
        </div>
    </form>
</div>
<% include footer.ejs %>